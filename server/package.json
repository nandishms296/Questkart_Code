{
  "name": "nodejs-express-sequelize-mysql",
  "version": "1.0.0",
  "description": "Node.js Rest Apis with Express, Sequelize & MySQL.",
  "main": "server.js",
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "scripts": {
    "start": "node server",
    "dev": "nodemon server",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:reset": "npx sequelize-cli db:migrate:undo:all && npm run migrate",
    "test": "jest --config=jest.config.js --verbose=false",
    "pretest": "cross-env NODE_ENV=test npm run migrate:reset"
  },
  "keywords": [
    "nodejs",
    "express",
    "sequelize",
    "mysql",
    "rest",
    "api"
  ],
  "author": "prasanna",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.332.0",
    "@aws-sdk/util-base64-node": "^3.209.0",
    "@octokit/rest": "^19.0.7",
    "basic-auth": "^2.0.1",
    "bcrypt": "^5.1.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "danfojs-node": "^1.1.2",
    "data-forge": "^1.10.0",
    "data-forge-fs": "^0.0.9",
    "dotenv": "^16.0.3",
    "exceljs": "^4.3.0",
    "express": "^4.18.2",
    "jest-extended": "^3.2.4",
    "jsforce": "^1.11.0",
    "jsonwebtoken": "^8.5.1",
    "mssql": "^9.1.0",
    "mysql2": "^2.3.3",
    "nodemailer": "^6.9.1",
    "oracledb": "^5.5.0",
    "pg": "^8.8.0",
    "pg-promise": "^11.0.2",
    "sequelize": "^6.25.2",
    "snowflake-sdk": "^1.6.17",
    "ssh2": "^1.11.0",
    "supertest": "^6.3.3",
    "urlsafe-base64": "^1.0.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.20",
    "prettier": "2.8.4"
  }
}
